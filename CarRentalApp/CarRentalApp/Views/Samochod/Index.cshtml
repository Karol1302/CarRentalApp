@model IEnumerable<CarRentalApp.Models.Samochod>

<head>
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Nasze Samochody</h2>
        </div>
    </div>
    <br />

    @if (Model.Count() > 0)
    {

        @foreach (var item in Model)
                {
                    <div id="myDiv" style="background-color: aliceblue; width:100%; height:200px" >
                        @item.Nazwa
                        @item.TypSamochodu

                    </div>
                    <br/>
                }


    }




</div>